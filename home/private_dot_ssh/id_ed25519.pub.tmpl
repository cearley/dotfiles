{{ $computerName := output "scutil" "--get" "ComputerName" | trim }}
{{- if contains "Mac Studio" $computerName -}}
{{- keepassxcAttribute "SSH (Mac Studio)" "id_ed25519.pub" -}}
{{ else if contains "MacBook Pro" $computerName -}}
{{- keepassxcAttribute "SSH (MacBook Pro)" "id_ed25519.pub" -}}
{{- end }}
