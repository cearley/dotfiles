	"shellcheck.ignorePatterns": {
		"**/*.zsh": false,
		"**/*.zshrc": false,
		"**/zshrc": false,
		"**/*.zprofile": false,
		"**/zprofile": false,
		"**/*.zlogin": false,
		"**/zlogin": false,
		"**/*.zlogout": false,
		"**/zlogout": false,
		"**/*.zshenv": false,
		"**/zshenv": false,
		"**/*.zsh-theme": false
